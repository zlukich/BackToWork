@model DbResultViewModel
@{
    ViewData["Title"] = "OrdererPage";
}
<div><h2>Индивидуальная страничка заказчика @Model.exemplars[0][1]</h2></div>
<table style="width:100%">
    @{
        foreach (string str in Model.column_names)
        {
            <td>@str</td>
        }
    }

    @{
        foreach (object[] exmpl in Model.exemplars)
        {
            <tr>
                @foreach (object s in exmpl)
                {

                    <td>@s</td>
                }
            </tr>
        }
    }
    @if (ViewBag.Owner == true)
    {
        <div><a class="btn btn-sm btn-primary" href="~/Home/EditOrdererPage/@Model.exemplars[0][1]">Edit</a></div>
    }
    else
    {

    }
</table>

