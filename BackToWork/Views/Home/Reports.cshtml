@model DbResultViewModel
@{
    ViewData["Title"] = "Index";
}

<table style="width:100%">
    @{ 
        <tr>
            @foreach (string str in Model.column_names)
            {
            <td>@str</td>
            }
        </tr>
    }
    
    @{ 
        foreach(object[] exmpl in Model.exemplars)
        {

        <tr>
            @for (int i = 0; i < exmpl.Length; i++)
            {
                @if (i == 1)
                {
                    <td><a href="~/Home/Orders/@exmpl[i]">@exmpl[i]</a></td>
                }
                else
                {
                    <td>@exmpl[i]</td>
                }


            }
            <td><a class="btn btn-sm btn-danger" href="~/Home/DeleteOrder/@exmpl[1]" role="button">Delete Order</a></td>
        </tr>
        }
    }

</table>